{
    "RSI": {
        "name": "RSI",
        "type": "talib",
        "required_inputs": ["close"],
        "params": {
            "timeperiod": {
                "type": "int",
                "min": 2,
                "max": 200,
                "default": 14,
                "description": "RSI period"
            }
        },
        "conditions": []
    },
    "MACD": {
        "name": "MACD",
        "type": "talib",
        "required_inputs": ["close"],
        "params": {
            "fastperiod": {
                "type": "int",
                "min": 2,
                "max": 200,
                "default": 12,
                "description": "Fast period"
            },
            "slowperiod": {
                "type": "int",
                "min": 2,
                "max": 200,
                "default": 26,
                "description": "Slow period"
            },
            "signalperiod": {
                "type": "int",
                "min": 2,
                "max": 200,
                "default": 9,
                "description": "Signal period"
            }
        },
        "conditions": [
            {
                "fastperiod": {
                    "lt": "slowperiod"
                }
            }
        ]
    },
